{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowJs": true,
    "module": "ESNext",
    "target": "ES5",
    "moduleResolution": "node",
    "strict": true,
    "declarationDir": "dist/types",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["ES2020", "DOM"] // 불필요한 타입 제거,
  }
}
